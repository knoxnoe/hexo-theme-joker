- const socialLink = theme.social_icon
#joker-footer
  ul.social
    each val, key in socialLink
      if(val !== null)
        li
          a(href=val) 
            i.fa(class='fa-' + key aria-hidden="true")
  .info
    p="Copyright Â© Blog 2021"
    p="Powered by Blog" 
